<?xml version="1.0"?>
<extension key="remote_expense" type="module">
  <file>remote_expense</file>
  <name>CiviRemote Expense</name>
  <description>Exposes CiviCRM expenses via the CiviRemote framework.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>info@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/remote_expense</url>
    <url desc="Documentation">https://github.com/systopia/remote_expense</url>
    <url desc="Support">https://github.com/systopia/remote_expense</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate/>
  <version>0.1-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.75</ver>
  </compatibility>
  <requires>
    <ext>expenses</ext>
    <ext>de.systopia.remotetools</ext>
  </requires>
  <comments/>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/RemoteExpense</namespace>
    <format>25.01.1</format>
    <angularModule>crmRemoteExpense</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
</extension>
